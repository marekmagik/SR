    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/title_activity_main" />
        <activity
            android:name=".SensorsTestActivity"
            android:label="@string/title_activity_sensors_test"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ServerTestActivity"
            android:label="@string/title_activity_server_test" >
        </activity>

        <service android:name=".sensors.GPSTrackerService" />


    </application>

</manifest>
